{
	"version": 2,
	"name": "github-website",
	"regions": [
		"sfo1",
		"bru"
	],
	"alias": [
		"bycedric.now.sh",
		"bycedric.com",
		"bycedric.dev",
		"cedric.dev",
		"finest.dev",
		"dekoningdernederlanden.nl"
	],
	"build": {
		"env": {
			"GITHUB_TOKEN": "@github-website-token",
			"GITHUB_LOGIN": "bycedric",
			"TWITTER_LOGIN": "cedric_dev",
			"DEVTO_LOGIN": "bycedric"
		}
	},
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/api/$1",
			"continue": true,
			"headers": {
				"cache-control": "s-maxage=86400"
			}
		}
	]
}
